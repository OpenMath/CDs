            <OMS cd="logic1" name="and"/>
            <OMA>
              <OMS cd="set1" name="in"/>
              <OMV name="a"/>
              <OMA>
                <OMS cd="algebraic_cats" name="integral_domain_set"/>
                <OMV name="S"/>
              </OMA>
            </OMA>
            <OMA>
              <OMS cd="set1" name="in"/>
