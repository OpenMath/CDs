            </OMA>
            <OMA>
              <OMA>
                <OMS cd="algebraic_cats" name="ringoid_plus"/>
                <OMV name="S"/>
              </OMA>
              <OMA>
                <OMA>
                  <OMS cd="algebraic_cats" name="ringoid_times"/>
                  <OMV name="S"/>
                </OMA>
                <OMV name="a"/>
                <OMV name="b"/>
              </OMA>
              <OMA>
                <OMA>
                  <OMS cd="algebraic_cats" name="ringoid_times"/>
                  <OMV name="S"/>
                </OMA>
                <OMV name="a"/>
                <OMV name="c"/>
              </OMA>
            </OMA>
          </OMA>
        </OMA>
      </OMBIND>
    </OMA>
  </OMOBJ></FMP>

  <CMP> (b+c)*a = b*a+c*a </CMP>

  <FMP><OMOBJ xmlns="http://www.openmath.org/OpenMath">
    <OMA><OMS cd="logic1" name="implies"/>
      <OMA>
        <OMS cd="set1" name="in"/>
