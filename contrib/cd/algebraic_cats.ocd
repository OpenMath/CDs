      </OMA>
      <OMA>
        <OMS cd="logic1" name="and"/>
        <OMBIND>
          <OMS cd="quant1" name="forall"/>
          <OMBVAR>
            <OMV name="a"/>
            <OMV name="b"/>
          </OMBVAR>
          <OMA>
            <OMS cd="logic1" name="implies"/>
            <OMA>
              <OMS cd="logic1" name="and"/>
              <OMA>
                <OMS cd="set1" name="in"/>
                <OMV name="a"/>
                <OMA>
                  <OMS cd="algebraic_cats" name="ordered_ring_set"/>
                  <OMV name="S"/>
                </OMA>
              </OMA>
              <OMA>
                <OMS cd="set1" name="in"/>
                <OMV name="b"/>
                <OMA>
                  <OMS cd="algebraic_cats" name="ordered_ring_set"/>
                  <OMV name="S"/>
                </OMA>
              </OMA>
            </OMA>
            <OMA>
              <OMS cd="logic1" name="or"/>
              <OMA>
                <OMA>
                  <OMS cd="algebraic_cats" name="ordered_ring_order"/>
                  <OMV name="S"/>
                </OMA>
                <OMV name="a"/>
                <OMV name="b"/>
              </OMA>
              <OMA>
                <OMA>
                  <OMS cd="algebraic_cats" name="ordered_ring_order"/>
                  <OMV name="S"/>
                </OMA>
                <OMV name="b"/>
                <OMV name="a"/>
              </OMA>
            </OMA>
          </OMA>
        </OMBIND>
        <OMBIND>
          <OMS cd="quant1" name="forall"/>
          <OMBVAR>
            <OMV name="a"/>
            <OMV name="b"/>
            <OMV name="c"/>
          </OMBVAR>
          <OMA>
            <OMS cd="logic1" name="implies"/>
            <OMA>
              <OMS cd="logic1" name="and"/>
              <OMA>
                <OMS cd="set1" name="in"/>
                <OMV name="a"/>
                <OMA>
                  <OMS cd="algebraic_cats" name="ordered_ring_set"/>
                  <OMV name="S"/>
                </OMA>
              </OMA>
              <OMA>
                <OMS cd="set1" name="in"/>
                <OMV name="b"/>
                <OMA>
                  <OMS cd="algebraic_cats" name="ordered_ring_set"/>
                  <OMV name="S"/>
                </OMA>
              </OMA>
              <OMA>
                <OMS cd="set1" name="in"/>
                <OMV name="c"/>
                <OMA>
