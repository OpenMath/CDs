                <OMV name="S"/>
              </OMA>
            </OMA>
            <OMA>
              <OMS cd="set1" name="in"/>
              <OMV name="b"/>
              <OMA>
                <OMS cd="algebraic_cats" name="non_commutative_ring_set"/>
