                <OMA>
                  <OMS cd="algebraic_cats" name="ordered_monoid_set"/>
                  <OMV name="S"/>
                </OMA>
              </OMA>
            </OMA>
            <OMA>
              <OMS cd="logic1" name="implies"/>
              <OMA>
                <OMS cd="logic1" name="and"/>
                <OMA>
                  <OMA>
                  <OMS cd="algebraic_cats" name="ordered_monoid_order"/>
                  <OMV name="S"/>
                </OMA>
                  <OMV name="a"/>
                  <OMV name="b"/>
                </OMA>
                <OMA>
                  <OMA>
                  <OMS cd="algebraic_cats" name="ordered_monoid_order"/>
                  <OMV name="S"/>
                </OMA>
                  <OMV name="b"/>
